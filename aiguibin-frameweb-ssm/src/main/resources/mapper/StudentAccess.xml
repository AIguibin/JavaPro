<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aiguibin.access.StudentAccess">
    <insert id="insertStudent" useGeneratedKeys="true"
            keyProperty="id" parameterType="com.aiguibin.entities.StudentEntity">
         insert into student (student_num,student_name,student_pass,student_date) values (#{studentNum},#{studentName},#{studentPass},#{studentDate})
    </insert>
	<select id="getAllStudent" resultType="com.aiguibin.beans.StudentBean">
		SELECT s.student_name,i.* FROM student s,student_info i WHERE s.student_num=i.student_num;
	</select>
    <select id="getStudentEntityByNum" resultType="com.aiguibin.entities.StudentEntity">
        select  * from  student where student_num=#{studentNum};
    </select>
</mapper>